{
	"_version": "1.0.0",
	"sap.app": {
		"id": "z.suek.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"handleValidation": true,
		"rootView": {
			"viewName": "z.suek.app.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.f": {},
				"sap.ui.comp":{},
				"sap.ui.table":{},
				"sap.ui.export":{}
			}
        },
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "z.suek.app.i18n.i18n"
				}
			},
			"local": {
				"type": "sap.ui.model.json.JSONModel",
				"settings":{
					"columns":[{
						"label": "{{taskName}}",
						"field": "task",
						"visible": true
					},{
						"label": "{{taskType}}",
						"field": "taskType",
						"visible": true
					},{
						"label": "{{responsible}}",
						"field": "responsible",
						"visible": true
					},{
						"label": "{{startDate}}",
						"field": "startDate",
						"visible": true
					},{
						"label": "{{endDate}}",
						"field": "endDate",
						"visible": true
					}],
					"taskTypes":[{
						"key": null,
						"text": ""
					},{
						"key": "task",
						"text": "{{task}}"
					},{
						"key": "bug",
						"text": "{{bug}}"
					},{
						"key": "changeRequest",
						"text": "{{changeRequest}}"
					},{
						"key": "story",
						"text": "{{story}}"
					}],
					"data": [
						{
						  "task": "Некорректно производится расчет для",
						  "taskType": "bug",
						  "responsible": "Ford Andrey",
						  "startDate": "20211201",
						  "endDate": "20220410",
						  "isNew": false,
						  "taskTypeText": "Исправление бага"
						},
						{
						  "task": "Не обновляется статус после согласования",
						  "taskType": "bug",
						  "responsible": "Johnson John",
						  "startDate": "20211201",
						  "endDate": "20220410",
						  "isNew": false,
						  "taskTypeText": "Исправление бага"
						},
						{
						  "task": "Установить update 1.2 на сервер приложений",
						  "taskType": "changeRequest",
						  "responsible": "Ford Andrey",
						  "startDate": "20211201",
						  "endDate": "20220410",
						  "taskTypeText": "Запрос на обслуживание"
						}
					  ],
					"state":{ 
						"edit": false
					},
					"persons": [{
						"name": "Johnson John"
					},{
						"name": "Ivanov Ivan"
					},{
						"name": "Smith Tom"
					},{
						"name": "Ford Andrey"
					}],
					"cols":[{
						"label":"{{name}}",
						"template": "name"
					}]
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "z.suek.app.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "/:?query:",
					"name": "main",
					"target": "main"
				}
			],
			"targets": {
				"main": {
					"viewName": "Main",
					"viewLevel": 1,
					"viewId": "main"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		}
	}
}